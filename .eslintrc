{

  "extends": "eslint:recommended",

  "rules": {
    "accessor-pairs": [2, { "getWithoutSet": false, "setWithoutGet": true }],
    "arrow-parens": [2, "always"],
    "arrow-spacing": [2, { "before": true, "after": true }],
    "block-scoped-var": 2,
    "callback-return": 0,
    "camelcase": [2, { "properties": "never" }],
    "comma-spacing": [2, { "before": false, "after": true }],
    "complexity": 0,
    "consistent-return": 2,
    "constructor-super": 2,
    "curly": [2, "multi-line"],
    "default-case": 0,
    "dot-location": [2, "property"],
    "dot-notation": [2, { "allowKeywords": true, "allowPattern": "" }],
    "eol-last": 2,
    "eqeqeq": 2,
    "generator-star-spacing": [2, { "before": true, "after": false }],
    "global-require": 0,
    "guard-for-in": 0,
    "handle-callback-err": 0,
    "init-declarations": [2, "always"],
    "key-spacing": [2, { "beforeColon": false, "afterColon": true }],
    "new-cap": [2, { "capIsNewExceptions": ["Router"] }],
    "new-parens": 2,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-class-assign": 2,
    "no-console": 0,
    "no-const-assign": 2,
    "no-delete-var": 2,
    "no-div-regex": 2,
    "no-dupe-class-members": 2,
    "no-else-return": 0,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-parens": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 0,
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-mixed-requires": ["error", { "allowCall": true }],
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": [2, { "props": false }],
    "no-path-concat": 2,
    "no-process-env": 0,
    "no-process-exit": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-restricted-modules": 0,
    "no-return-assign": [2, "always"],
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": [2, { "builtinGlobals": true }],
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-sync": 0,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undef-init": 0,
    "no-undefined": 2,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 2,
    "no-unused-vars": [2, { "vars": "all", "args": "none" }],
    "no-use-before-define": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-var": 0,
    "no-void": 2,
    "no-warning-comments": 0,
    "no-with": 2,
    "object-shorthand": 0,
    "prefer-arrow-callback": 1,
    "prefer-const": 1,
    "prefer-reflect": 0,
    "prefer-spread": 1,
    "prefer-template": 2,
    "quotes": [2, "single", "avoid-escape"],
    "radix": 2,
    "require-yield": 0,
    "semi": 2,
    "semi-spacing": [2, { "before": false, "after": true }],
    "space-infix-ops": 2,
    "keyword-spacing": 2,
    "space-unary-ops": 1,
    "strict": [2, "global"],
    "vars-on-top": 0,
    "wrap-iife": [2, "inside"],
    "yoda": 0
  },

  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },

  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "destructuring": true,
      "forOf": false,
      "generators": false,
      "modules": true,
      "objectLiteralComputedProperties": true,
      "objectLiteralDuplicateProperties": false,
      "objectLiteralShorthandMethods": true,
      "objectLiteralShorthandProperties": true,
      "octalLiterals": false,
      "regexUFlag": false,
      "regexYFlag": false,
      "restParams": true,
      "spread": true,
      "superInFunctions": true,
      "templateStrings": true,
      "unicodeCodePointEscapes": true
    }
  },
}
